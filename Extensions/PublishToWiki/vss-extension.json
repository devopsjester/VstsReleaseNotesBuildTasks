{
  "manifestVersion": 1,
  "id": "PremDev-VSTS-PublishToWiki-Task",
  "version": "0.1.33",
  "name": "Publish Release Notes to Project Wiki Task",
  "publisher": "DevManWalking",
  "description": "Custom task that publishes a release-notes markdown file to the wiki.",
  "categories": [
    "Build and release"
  ],
  "targets": [{
    "id": "Microsoft.VisualStudio.Services"
  }],
  "tags": [
    "Release",
    "Package",
    "Publish"
  ],
  "branding": {
    "color": "#f97923",
    "theme": "dark"
  },
  "icons": {
    "default": "images/logo.png"
  },
  "links": {
    "support": {
      "uri": "https://support.microsoft.com/en-us/premier/contacts"
    }
  },
  "screenshots": [{
      "path": "images/screenshot.png"
    },
    {
      "path": "images/screenshot1.png"
    }
  ],
  "content": {
    "details": {
      "path": "readme.md"
    },
    "license": {
      "path": "license.md"
    }
  },
  "files": [{
    "path": "PublishToWikiTask"
  }],
  "contributions": [{
    "id": "Publish-To-Wiki",
    "type": "ms.vss-distributed-task.task",
    "targets": [
      "ms.vss-distributed-task.tasks"
    ],
    "properties": {
      "name": "PublishToWikiTask"
    }
  }]
}